{
  "name": "drupal/ai_connect",
  "description": "AI Connect for Drupal - WebMCP Protocol Bridge",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4",
    "drupal/core": "^9 || ^10 || ^11",
    "firebase/php-jwt": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\ai_connect\\": "src/"
    }
  },
  "config": {
    "audit": {
      "block-insecure": false
    }
  },
  "require-dev": {
    "mglaman/drupal-check": "*"
  }
}
